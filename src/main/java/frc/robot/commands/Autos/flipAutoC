package frc.robot.commands.Autos;
import frc.robot.subsystems.tankDriveS;
import edu.wpi.first.wpilibj.Timer;
import edu.wpi.first.wpilibj2.command.CommandBase;


public class flipAutoC extends CommandBase {
long timeStart;
double autoSpeed;
    public final tankDriveS drive;
    public basicAuto(tankDriveS subsystem, double MotorSpeed,int degrees ){
        drive = subsystem;
        autoSpeed = MotorSpeed;
        Rotation2d desiredAngle = Rotation2d.fromDegrees(degrees);
        addRequirements(subsystem);
        
    }
    public void initialize(){
        System.out.print("Running flip...");
    }
    public void execute() {
        while (gyro.getAngle().degrees()<desiredAngle){
            subsystem.arcadeDrive(0,turnSpeed);
        }
        
    }
    public void end() {
        drive.tankDrive(0,0);
        drive.runStop();
    }
}
